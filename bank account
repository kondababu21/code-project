class BankAccount: 
def __init__(self, account_number, holder, balance=0): 
self.account_number = account_number 
self.holder = holder 
self.balance = balance 
def deposit(self, amount): 
self.balance += amount 
print(f"Deposited {amount}. New Balance = 
{self.balance}") 
def withdraw(self, amount): 
if amount <= self.balance: 
self.balance -= amount 
print(f"Withdrew {amount}. New Balance = 
{self.balance}") 
else: 
print("Insufficient Balance!") 
# Objects 
acc1 = BankAccount(12345, "Ravi", 5000) 
acc1.deposit(2000) 
acc1.withdraw(3000) 
acc1.withdraw(6000)
