class Student:
    def __init__(self, marks):
        self.marks = marks

    def __gt__(self, other):  # greater than
        return self.marks > other.marks

    def __eq__(self, other):  # equal
        return self.marks == other.marks

# Test
s1 = Student(85)
s2 = Student(90)
s3 = Student(85)

print("s1 > s2 ?", s1 > s2)
print("s1 == s3 ?", s1 == s3)
Output:
s1 > s2 ? False
s1 == s3 ? True
